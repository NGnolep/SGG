  fishName: Narwhal
  fishDescription: 'Scientific Name: Monodon monoceros Diet: Carnivore Lifespan:
    25-50 years Class: Mammalia'
  fishImage: {fileID: 1646928479, guid: e39fc6cfafc019145833ab06be92606a, type: 3}
